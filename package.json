{
  "name": "spectrum",
  "version": "0.0.0",
  "description": "Spectrum protocol server and client for Node.js",
  "main": "index.js",
  "scripts": {
    "build": "./node_modules/.bin/tsc",
    "coverage": "nyc mocha test",
    "test": "node ./node_modules/typescript/bin/tsc -p tests.json && mocha test",
    "docs": "typedoc --out docs src",
    "gh-pages": "rimraf docs && npm run docs && gh-pages -d docs",
    "coveralls": "nyc mocha test && cat ./coverage/lcov.info | node ./node_modules/coveralls/bin/coveralls.js",
    "build:watch": "tsc --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hskang9/Spectrum.git"
  },
  "keywords": [
    "javascript",
    "beam",
    "mimblewimble",
    "rpc",
    "tcp",
    "http",
    "grin",
    "socket",
    "server",
    "jsonrpc",
    "jsonrpc2",
    "json-rpc2",
    "spectrum",
    "daemon",
    "protocol",
    "altcoin",
    "cryptocurrency",
    "privacy"
  ],
  "author": "Nick Hyungsuk Kang",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/hskang9/Spectrum/issues"
  },
  "homepage": "https://github.com/hskang9/Spectrum#readme",
  "devDependencies": {
    "@types/chai": "^4.0.10",
    "@types/mocha": "^2.2.44",
    "@types/sinon": "^4.1.2",
    "chai": "^4.1.2",
    "coveralls": "^3.0.0",
    "gh-pages": "^2.0.1",
    "mocha": "^6.0.2",
    "nyc": "^13.3.0",
    "sinon": "^4.1.3",
    "typedoc": "^0.14.2",
    "typescript": "^3.3.3333"
  },
  "dependencies": {
    "@types/bluebird": "^3.5.18",
    "@types/commander": "^2.12.2",
    "@types/debug": "^0.0.30",
    "@types/lodash": "^4.14.122",
    "@types/uuid": "^3.4.3",
    "better-curry": "^1.6.0",
    "bluebird": "^3.5.1",
    "chalk": "^2.3.0",
    "child_process": "^1.0.2",
    "commander": "^2.12.2",
    "debug": "^3.1.0",
    "eventemitter3": "^3.0.0",
    "lodash": "^4.17.11",
    "source-map-support": "^0.5.0",
